{
  "name": "@blackbolt/api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": "24.13.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.api.js",
    "start:api": "node dist/main.api.js",
    "start:worker": "node dist/main.worker.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint --ext .ts src",
    "test": "jest --config jest.config.ts --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/bullmq": "11.0.1",
    "@nestjs/common": "11.0.3",
    "@nestjs/core": "11.0.3",
    "@nestjs/platform-express": "11.0.3",
    "@prisma/client": "6.19.2",
    "bullmq": "5.69.2",
    "csv-parse": "5.6.0",
    "ioredis": "5.4.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "11.0.3",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.1",
    "@types/supertest": "6.0.2",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prisma": "6.19.2",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.7.2"
  }
}
