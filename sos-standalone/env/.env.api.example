NODE_ENV=production
PORT=3000

DATABASE_URL=postgresql://<user>:<pass>@<host>:<port>/<db>
REDIS_URL=redis://<user>:<pass>@<host>:<port>

# Stripe
STRIPE_SECRET_KEY=sk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...

# SOS tenant routing
TENANT_ID=<sos-tenant-id>

# Follow-up email provider
SOS_POSTMARK_SERVER_TOKEN=<postmark-server-token>
SOS_POSTMARK_FROM_EMAIL=<verified-sender@example.com>

# Provider fax
SOS_FAX_PROVIDER=srfax
SOS_SRFAX_BASE_URL=https://www.srfax.com/SRF_SecWebSvc.php
SOS_SRFAX_ACCOUNT_ID=<account-id>
SOS_SRFAX_PASSWORD=<password>
SOS_SRFAX_SENDER_NUMBER=<sender-number>

# Optional monitor webhook for failure notifications
SOS_MONITOR_WEBHOOK_URL=
